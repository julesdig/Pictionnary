{% extends 'base.html.twig' %}

{% block title %}{{ "security.register"|trans() }}{% endblock %}

{% block body %}
<div class=" d-flex justify-content-center align-items-center vh-100">
    {{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.firstName) }}
    {{ form_row(form.lastName) }}
    {{ form_row(form.password) }}
    {{ form_row(form.save) }}
    {{ form_end(form) }}
</div>

{% endblock %}



